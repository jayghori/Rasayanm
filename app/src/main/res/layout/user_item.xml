<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:layout_margin="5dp"
    android:padding="10dp"
    xmlns:app="http://schemas.android.com/apk/res-auto">


<androidx.cardview.widget.CardView
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:ellipsize="end"
    android:padding="5dp"
    app:cardCornerRadius="5dp"
    app:cardUseCompatPadding="true">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:padding="16dp">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:src="@drawable/ic_user_logo"
            android:layout_alignParentTop="true"
            android:layout_marginRight="20dp" />

        <TextView
            android:id="@+id/title"
            android:textColor="@color/black"
            android:layout_width="match_parent"
            android:textIsSelectable="true"
            android:fontFamily="sans-serif-medium"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_toRightOf="@+id/imageView"
            android:text="Name"
            android:textSize="20dp" />


        <TextView
            android:layout_width="match_parent"
            android:id="@+id/tvName"
            android:textIsSelectable="true"
            android:fontFamily="sans-serif-medium"
            android:textColor="@color/black"
            android:text="User Name"
            android:layout_below="@+id/title"
            android:layout_toRightOf="@+id/imageView"
            android:layout_height="wrap_content"/>

    </RelativeLayout>


</androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_width="match_parent"
        app:cardUseCompatPadding="true"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:padding="16dp">

            <ImageView
                android:id="@+id/imageView1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:src="@drawable/ic_email"
                android:layout_alignParentTop="true"
                android:layout_marginRight="20dp" />

            <TextView
                android:id="@+id/title1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="sans-serif-medium"
                android:layout_alignParentTop="true"
                android:textColor="@color/black"
                android:layout_toRightOf="@+id/imageView1"
                android:text="Email"
                android:textSize="20dp" />


            <TextView
                android:layout_width="wrap_content"
                android:id="@+id/tvEmail"
                android:text="User Email"
                android:fontFamily="sans-serif-medium"
                android:textColor="@color/black"
                android:layout_below="@+id/title1"
                android:layout_toRightOf="@+id/imageView1"
                android:layout_height="wrap_content"/>

        </RelativeLayout>


    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_width="match_parent"
        app:cardUseCompatPadding="true"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:padding="16dp">

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:src="@drawable/ic_phonenumber"
                android:layout_alignParentTop="true"
                android:layout_marginRight="20dp" />

            <TextView
                android:id="@+id/title2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:fontFamily="sans-serif-medium"
                android:layout_alignParentTop="true"
                android:layout_toRightOf="@+id/imageView2"
                android:text="Phone-Number"
                android:textSize="20dp" />


            <TextView
                android:layout_width="wrap_content"
                android:id="@+id/tvPhoneNumber"
                android:text="User Email"
                android:textColor="@color/black"
                android:layout_below="@+id/title2"
                android:fontFamily="sans-serif-medium"
                android:layout_toRightOf="@+id/imageView2"
                android:layout_height="wrap_content"/>

        </RelativeLayout>


    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_width="match_parent"
        app:cardUseCompatPadding="true"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:padding="16dp">

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:src="@drawable/ic_location"
                android:layout_alignParentTop="true"
                android:layout_marginRight="20dp" />

            <TextView
                android:id="@+id/title3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:textColor="@color/black"
                android:fontFamily="sans-serif-medium"
                android:layout_toRightOf="@+id/imageView3"
                android:text="Address"
                android:textSize="20dp" />


            <TextView
                android:layout_width="wrap_content"
                android:id="@+id/tvAddress"
                android:text="User Email"
                android:fontFamily="sans-serif-medium"
                android:textColor="@color/black"
                android:layout_below="@+id/title3"
                android:layout_toRightOf="@+id/imageView3"
                android:layout_height="wrap_content"/>

        </RelativeLayout>


    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_width="match_parent"
        app:cardUseCompatPadding="true"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:padding="16dp">

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:src="@drawable/ic_gst_number"
                android:layout_alignParentTop="true"
                android:layout_marginRight="20dp" />

            <TextView
                android:id="@+id/title4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:fontFamily="sans-serif-medium"
                android:textColor="@color/black"
                android:layout_toRightOf="@+id/imageView4"
                android:text="GST-Number"
                android:textSize="20dp" />


            <TextView
                android:layout_width="wrap_content"
                android:id="@+id/tvGstNumber"
                android:text="User Email"
                android:fontFamily="sans-serif-medium"
                android:textColor="@color/black"
                android:layout_below="@+id/title4"
                android:layout_toRightOf="@+id/imageView4"
                android:layout_height="wrap_content"/>

        </RelativeLayout>


    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_width="match_parent"
        app:cardUseCompatPadding="true"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:padding="16dp">

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:src="@drawable/ic_pannumber"
                android:layout_alignParentTop="true"
                android:layout_marginRight="20dp" />

            <TextView
                android:id="@+id/title5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:textColor="@color/black"
                android:fontFamily="sans-serif-medium"
                android:layout_toRightOf="@+id/imageView5"
                android:text="PAN-Number"
                android:textSize="20dp" />


            <TextView
                android:layout_width="wrap_content"
                android:id="@+id/tvPanNumber"
                android:text="User Email"
                android:textColor="@color/black"
                android:fontFamily="sans-serif-medium"
                android:layout_below="@+id/title5"
                android:layout_toRightOf="@+id/imageView5"
                android:layout_height="wrap_content"/>

        </RelativeLayout>





    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_width="match_parent"
        app:cardUseCompatPadding="true"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:orientation="vertical"
            android:padding="16dp">


           <TextView
               android:layout_width="wrap_content"
               android:id="@+id/cancleCheck"
               android:text="Cancle-Check"
               android:textColor="@color/black"
               android:fontFamily="sans-serif-medium"
               android:textSize="10dp"
               android:layout_height="wrap_content"/>

            <ImageView
                android:id="@+id/img"
                android:layout_marginTop="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_gst_number"
                android:layout_marginRight="20dp" />

        </LinearLayout>

    </androidx.cardview.widget.CardView>


